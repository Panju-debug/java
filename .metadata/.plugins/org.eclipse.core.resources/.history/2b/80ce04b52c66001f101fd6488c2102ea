		Auto auto1= new Auto();
		Auto auto2= new Auto();
		
		auto1.marca="Porche";
		auto1.anio=2024;
		auto1.precio=50000.00;
		
		auto2.marca="Kia";
		auto2.anio=2020;
		auto2.precio=16000.00;
		
		System.out.println("marca:"+" "+auto1);
	}

}
