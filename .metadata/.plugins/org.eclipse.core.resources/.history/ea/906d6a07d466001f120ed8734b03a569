package com.krakedev;

public class TestRectangulo {

	public static void main(String[] args) {
		int areaR1;
		Rectangulo r1= new Rectangulo();
		Rectangulo r2=new Rectangulo();
		
		r1.base=10;
		r1.altura=5;
		
		r2.base=8;
		r2.altura=3;
		
		areaR1=r1.calcularArea();
		System.out.println("Area de r1="+" "+areaR1);
	}

}
